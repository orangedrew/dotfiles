export EDITOR=nvim
export PATH=$PATH:/snap/bin
export KUBECONFIG=~/.kube/config
export KUBE_TEST='ssh -fNL 6448:10.101.2.71:6443 laurens.druwel@synaps.lan@synapsjs01.synaps.lan'
export KUBE_SCECNNB='ssh -fNL 8080:10.200.2.100:22 laurens.druwel@synaps.lan@synapsjs01.synaps.lan && ssh -fND 9401 soladmin@127.0.0.1 -p 8080 && ssh -fNT -L 6445:10.101.2.220:6443 laurens.druwel@synaps.lan@synapsjs01.synaps.lan'
